# POE Integration Environment Variables
# Copy this file to .env and update with your actual values

# POE API Configuration
REACT_APP_POE_API_URL=https://api.poe.com/v1
REACT_APP_POE_API_KEY=your_poe_api_key_here
REACT_APP_POE_BOT_ID=your_bot_id_here

# Optional: Model Configuration
REACT_APP_POE_MODEL=claude-3-sonnet
REACT_APP_POE_TIMEOUT=30000

# Debug Mode (set to true for development)
REACT_APP_DEBUG_POE=false
